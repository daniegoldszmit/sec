{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dg2adf"
		},
		"AzureSearch1_key": {
			"type": "secureString",
			"metadata": "Secure string for 'key' of 'AzureSearch1'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"LinkedServiceDatalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedServiceDatalake'"
		},
		"LinkedServiceFileServerOnPrem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LinkedServiceFileServerOnPrem'"
		},
		"LinkedServiceSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedServiceSqlServer'"
		},
		"PostgreSqlOnPrem2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PostgreSqlOnPrem2'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"AzureSearch1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dgcognitivesearch.search.windows.net"
		},
		"LinkedServiceFileServerOnPrem_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\data\\Northwind"
		},
		"LinkedServiceFileServerOnPrem_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "usr_datafactory"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dg2adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSearch1_key": {
						"value": "[parameters('AzureSearch1_key')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"LinkedServiceDatalake_connectionString": {
						"value": "[parameters('LinkedServiceDatalake_connectionString')]"
					},
					"LinkedServiceSqlServer_connectionString": {
						"value": "[parameters('LinkedServiceSqlServer_connectionString')]"
					},
					"LinkedServiceFileServerOnPrem_password": {
						"value": "[parameters('LinkedServiceFileServerOnPrem_password')]"
					},
					"PostgreSqlOnPrem2_connectionString": {
						"value": "[parameters('PostgreSqlOnPrem2_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"AzureSearch1_properties_typeProperties_url": {
						"value": "[parameters('AzureSearch1_properties_typeProperties_url')]"
					},
					"LinkedServiceFileServerOnPrem_properties_typeProperties_host": {
						"value": "[parameters('LinkedServiceFileServerOnPrem_properties_typeProperties_host')]"
					},
					"LinkedServiceFileServerOnPrem_properties_typeProperties_userId": {
						"value": "[parameters('LinkedServiceFileServerOnPrem_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dg2adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dg2adf_ArmTemplate_0')]"
			]
		}
	]
}