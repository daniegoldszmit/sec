{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dg2adf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/PipelineCopyDataFromOnPrem')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy to Azure Search",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "PostgreSqlSource"
							},
							"sink": {
								"type": "AzureSearchIndexSink",
								"writeBatchSize": 1000,
								"writeBehavior": "upload"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "PostgreSqlTablePersonAddress",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "AzureSearchIndex1",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "Copy Orders to DataLake Raw Data",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "FileServerReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "ParquetSink",
								"storeSettings": {
									"type": "AzureBlobStorageWriteSettings"
								},
								"formatSettings": {
									"type": "ParquetWriteSettings"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "ORDERID",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ORDERID",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "CUSTOMERID",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "CUSTOMERID",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "EMPLOYEEID",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "EMPLOYEEID",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "ORDERDATE",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ORDERDATE",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "REQUIREDDA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "REQUIREDDA",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPPEDDAT",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPPEDDAT",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPVIA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPVIA",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "FREIGHT",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "FREIGHT",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPNAME",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPNAME",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPADDRES",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPADDRES",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPCITY",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPCITY",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPREGION",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPREGION",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPPOSTAL",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPPOSTAL",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SHIPCOUNTR",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SHIPCOUNTR",
											"type": "String",
											"physicalType": "UTF8"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "Dataset_Orders_dbf",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "Dataset_Orders_Datalake",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/Dataset_Orders_dbf')]",
				"[concat(variables('factoryId'), '/datasets/Dataset_Orders_Datalake')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Dataset_Orders_Datalake')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedServiceDatalake",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"folderPath": "Northwind",
						"container": "datalake-raw-data"
					},
					"compressionCodec": "snappy"
				},
				"schema": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Dataset_Orders_dbf')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedServiceFileServerOnPrem",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "FileServerLocation",
						"fileName": "orders.csv"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "ORDERID",
						"type": "String"
					},
					{
						"name": "CUSTOMERID",
						"type": "String"
					},
					{
						"name": "EMPLOYEEID",
						"type": "String"
					},
					{
						"name": "ORDERDATE",
						"type": "String"
					},
					{
						"name": "REQUIREDDA",
						"type": "String"
					},
					{
						"name": "SHIPPEDDAT",
						"type": "String"
					},
					{
						"name": "SHIPVIA",
						"type": "String"
					},
					{
						"name": "FREIGHT",
						"type": "String"
					},
					{
						"name": "SHIPNAME",
						"type": "String"
					},
					{
						"name": "SHIPADDRES",
						"type": "String"
					},
					{
						"name": "SHIPCITY",
						"type": "String"
					},
					{
						"name": "SHIPREGION",
						"type": "String"
					},
					{
						"name": "SHIPPOSTAL",
						"type": "String"
					},
					{
						"name": "SHIPCOUNTR",
						"type": "String"
					},
					{
						"name": "_NULLFLAGS",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LinkedServiceFileServerOnPrem')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/LinkedServiceFileServerOnPrem')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "FileServer",
				"typeProperties": {
					"host": "C:\\data\\Northwind",
					"userId": "usr_datafactory",
					"password": {
						"type": "SecureString",
						"value": "**********"
					}
				},
				"connectVia": {
					"referenceName": "integrationRuntime2",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": []
		}
	]
}