{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dg2adf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Dataset_Orders_Datalake')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedServiceDatalake",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"folderPath": "Northwind",
						"container": "datalake-raw-data"
					},
					"compressionCodec": "snappy"
				},
				"schema": [
					{
						"name": "ORDERID",
						"type": "UTF8"
					},
					{
						"name": "CUSTOMERID",
						"type": "UTF8"
					},
					{
						"name": "EMPLOYEEID",
						"type": "UTF8"
					},
					{
						"name": "ORDERDATE",
						"type": "UTF8"
					},
					{
						"name": "REQUIREDDA",
						"type": "UTF8"
					},
					{
						"name": "SHIPPEDDAT",
						"type": "UTF8"
					},
					{
						"name": "SHIPVIA",
						"type": "UTF8"
					},
					{
						"name": "FREIGHT",
						"type": "UTF8"
					},
					{
						"name": "SHIPNAME",
						"type": "UTF8"
					},
					{
						"name": "SHIPADDRES",
						"type": "UTF8"
					},
					{
						"name": "SHIPCITY",
						"type": "UTF8"
					},
					{
						"name": "SHIPREGION",
						"type": "UTF8"
					},
					{
						"name": "SHIPPOSTAL",
						"type": "UTF8"
					},
					{
						"name": "SHIPCOUNTR",
						"type": "UTF8"
					}
				]
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/Dataset_Orders_dbf')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LinkedServiceFileServerOnPrem",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "FileServerLocation",
						"fileName": "orders.csv"
					},
					"columnDelimiter": ",",
					"encodingName": "ISO-8859-1",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "ORDERID",
						"type": "String"
					},
					{
						"name": "CUSTOMERID",
						"type": "String"
					},
					{
						"name": "EMPLOYEEID",
						"type": "String"
					},
					{
						"name": "ORDERDATE",
						"type": "String"
					},
					{
						"name": "REQUIREDDA",
						"type": "String"
					},
					{
						"name": "SHIPPEDDAT",
						"type": "String"
					},
					{
						"name": "SHIPVIA",
						"type": "String"
					},
					{
						"name": "FREIGHT",
						"type": "String"
					},
					{
						"name": "SHIPNAME",
						"type": "String"
					},
					{
						"name": "SHIPADDRES",
						"type": "String"
					},
					{
						"name": "SHIPCITY",
						"type": "String"
					},
					{
						"name": "SHIPREGION",
						"type": "String"
					},
					{
						"name": "SHIPPOSTAL",
						"type": "String"
					},
					{
						"name": "SHIPCOUNTR",
						"type": "String"
					},
					{
						"name": "_NULLFLAGS",
						"type": "String"
					}
				]
			},
			"dependsOn": []
		}
	]
}